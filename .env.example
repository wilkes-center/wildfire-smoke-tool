# Copy this file to .env.local and configure your logging settings

# Existing configuration
REACT_APP_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
REACT_APP_CENSUS_API_KEY=your_census_api_key_here
REACT_APP_DEBUG=true
REACT_APP_TEST=working

# Logging Configuration
REACT_APP_LOG_LEVEL=DEBUG
REACT_APP_ENABLE_REMOTE_LOGGING=false
REACT_APP_LOG_ENDPOINT=https://your-logging-endpoint.com/api/logs

# Development Settings
REACT_APP_ENABLE_CONSOLE_LOGGING=true
REACT_APP_ENABLE_PERFORMANCE_TRACKING=true
REACT_APP_ENABLE_AUTO_LOG_MOUNTS=true

# Production Settings (override in .env.production)
# REACT_APP_LOG_LEVEL=WARN
# REACT_APP_ENABLE_REMOTE_LOGGING=true
# REACT_APP_ENABLE_CONSOLE_LOGGING=false
